arsak_tilsett = d_full %>%  dplyr::count(tilsett) %>% dplyr::mutate(pro = round(100*(n/sum(n))))

antall_svar = arsak_tilsett %>% dplyr::summarise(sum(n))
antall_pas = arsak_tilsett %>% dplyr::count(n)

